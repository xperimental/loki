{
  "Name": "lokistack-dev",
  "Namespace": "openshift-logging",
  "Image": "docker.io/grafana/loki:2.9.1",
  "GatewayImage": "quay.io/observatorium/api:latest",
  "GatewayBaseDomain": "cluster.example.com",
  "ConfigSHA1": "",
  "CertRotationRequiredAt": "",
  "Gates": {
    "serviceMonitors": true,
    "serviceMonitorTlsEndpoints": true,
    "lokiStackAlerts": true,
    "httpEncryption": true,
    "grpcEncryption": true,
    "builtInCertManagement": {
      "enabled": true,
      "caValidity": "43830h",
      "caRefresh": "35064h",
      "certValidity": "2160h",
      "certRefresh": "1728h"
    },
    "lokiStackGateway": true,
    "restrictedPodSecurityStandard": true,
    "defaultNodeAffinity": true,
    "openshift": {
      "enabled": true,
      "servingCertsService": true,
      "ruleExtendedValidation": true,
      "clusterTLSPolicy": true,
      "clusterProxy": true,
      "dashboards": true
    }
  },
  "Stack": {
    "managementState": "Managed",
    "size": "1x.demo",
    "storage": {
      "schemas": [
        {
          "version": "v12",
          "effectiveDate": "2022-06-01"
        }
      ],
      "secret": {
        "type": "s3",
        "name": "test"
      }
    },
    "storageClassName": "gp3-csi",
    "replication": {
      "factor": 1
    },
    "rules": {
      "enabled": true,
      "selector": {
        "matchLabels": {
          "openshift.io/cluster-monitoring": "true"
        }
      },
      "namespaceSelector": {
        "matchLabels": {
          "openshift.io/cluster-monitoring": "true"
        }
      }
    },
    "limits": {
      "global": {
        "ingestion": {
          "ingestionRate": 4,
          "ingestionBurstSize": 6,
          "maxLabelNameLength": 1024,
          "maxLabelValueLength": 2048,
          "maxLabelNamesPerSeries": 30,
          "maxLineSize": 256000,
          "perStreamRateLimit": 3,
          "perStreamRateLimitBurst": 15
        },
        "queries": {
          "maxEntriesLimitPerQuery": 5000,
          "maxChunksPerQuery": 2000000,
          "maxQuerySeries": 500,
          "queryTimeout": "3m",
          "cardinalityLimit": 100000
        }
      }
    },
    "template": {
      "compactor": {
        "replicas": 1
      },
      "distributor": {
        "replicas": 1
      },
      "ingester": {
        "replicas": 1
      },
      "querier": {
        "replicas": 1
      },
      "queryFrontend": {
        "replicas": 1
      },
      "gateway": {
        "replicas": 2
      },
      "indexGateway": {
        "replicas": 1
      },
      "ruler": {
        "replicas": 1
      }
    },
    "tenants": {
      "mode": "openshift-logging"
    }
  },
  "ResourceRequirements": {
    "IndexGateway": {
      "Limits": null,
      "Requests": null,
      "PVCSize": "10Gi",
      "PDBMinAvailable": 0
    },
    "Ingester": {
